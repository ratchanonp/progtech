package q1;

import java.util.Scanner;

/**
 *
 * @author ratchanon
 */
public class Q1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String input;
        String[] input_1;
        
        double aX, aY, aRadius;
        double bX, bY, bRadius;
        
        
        System.out.print("circle A: center and radius : ");
        input = scanner.nextLine();
        
        input_1 = input.replace("(", "").replace(")", "").replace(",", "").trim().split(" ");
        aX = Double.parseDouble(input_1[0]);
        aY = Double.parseDouble(input_1[1]);
        aRadius = Double.parseDouble(input_1[2]);
        
        
        System.out.println("output : " + input_1[0] );
        
        
    }
    
}
